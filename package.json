{
  "name": "rust-docs-sidebar",
  "displayName": "Rust Docs Sidebar",
  "description": "Displays Rust documentation for the selected symbol in a convenient sidebar with fast local search",
  "version": "0.0.1",
  "publisher": "maxwindiff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwindiff/rust-docs-sidebar"
  },
  "keywords": [
    "rust",
    "documentation",
    "rustdoc",
    "sidebar",
    "docs"
  ],
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages",
    "Documentation"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rust-docs-sidebar",
          "title": "Rust Docs Sidebar",
          "icon": "$(book)"
        }
      ]
    },
    "views": {
      "rust-docs-sidebar": [
        {
          "id": "rust-docs-sidebar.docsView",
          "name": "Documentation",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "highlight.js": "^11.11.1"
  }
}
